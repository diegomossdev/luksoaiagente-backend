  "version": "1.0.0",
  "description": "Backend API with Fastify, Supabase and OpenAI integration",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "echo 'Build completed - copying files' && mkdir -p dist && cp -r src dist/ && cp package.json dist/",
    "start:prod": "NODE_ENV=production node dist/src/server.js",
